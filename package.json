{
    "name": "nestjs-techniques-undici",
    "version": "0.0.6",
    "description": "Undici module example",
    "author": "Hebert Barros <hebert@hotbrains.com.br>",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "node": "20 || >=22",
        "npm": "9 || >=10.9.0"
    },
    "scripts": {
        "setup": "cp .env.example .env",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "doc": "npx @compodoc/compodoc -p tsconfig.json -s",
        "dev:test": "nodemon src/main.ts --debug --watch",
        "dev": "npm run dev:test",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare:enviroment": "node scripts/dev-enviroment.cjs",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "dependencies": {
        "@nestjs/common": "10.4.17",
        "@nestjs/core": "10.4.13",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "10.4.13",
        "@nestjs/serve-static": "4.0.2",
        "@nestjs/swagger": "7.4.2",
        "body-parser": "1.20.3",
        "chance": "^1.1.12",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "compression": "1.8.1",
        "cors": "2.8.5",
        "dotenv": "16.4.7",
        "express": "4.21.1",
        "express-async-errors": "3.1.1",
        "helmet": "5.1.1",
        "http-status": "1.8.1",
        "morgan": "1.10.1",
        "nest-shared": "^5.0.6",
        "nestjs-undici": "^0.1.54",
        "qs": "6.11.2",
        "reflect-metadata": "0.1.14",
        "rimraf": "3.0.2",
        "rxjs": "7.5.7",
        "swagger-ui-express": "4.6.3",
        "tsyringe": "4.8.0"
    },
    "devDependencies": {
        "@babel/cli": "7.25.9",
        "@babel/core": "7.26.0",
        "@babel/node": "7.26.0",
        "@babel/preset-env": "7.26.0",
        "@compodoc/compodoc": "1.1.26",
        "@nestjs/cli": "9.5.0",
        "@nestjs/schematics": "9.2.0",
        "@nestjs/testing": "10.4.13",
        "@types/body-parser": "1.19.5",
        "@types/cache-manager": "4.0.6",
        "@types/chance": "^1.1.6",
        "@types/compression": "1.7.5",
        "@types/cors": "2.8.17",
        "@types/express": "4.17.21",
        "@types/helmet": "0.0.48",
        "@types/jest": "27.5.2",
        "@types/moment": "2.13.0",
        "@types/mongoose": "5.11.97",
        "@types/morgan": "1.9.9",
        "@types/multer": "1.4.12",
        "@types/node": "^20.17.9",
        "@types/supertest": "2.0.16",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "@typescript-eslint/typescript-estree": "5.62.0",
        "eslint": "8.57.1",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "8.10.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-prettier": "4.2.1",
        "jest": "28.1.3",
        "nodemon": "2.0.22",
        "prettier": "2.8.8",
        "sucrase": "3.35.0",
        "supertest": "7.1.0",
        "ts-jest": "28.0.8",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "4.9.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
