{
    "name": "nestjs-techniques-undici",
    "version": "0.0.2",
    "description": "Undici module example",
    "author": "Hebert Barros <hebert@hotbrains.com.br>",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "setup": "cp .env.example .env",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "doc": "npx @compodoc/compodoc -p tsconfig.json -s",
        "dev:test": "nodemon src/main.ts --debug --watch",
        "dev": "npm run dev:test",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare:enviroment": "node scripts/dev-enviroment.cjs",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "dependencies": {
        "@nestjs/common": "8.4.7",
        "@nestjs/core": "8.4.7",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "8.4.7",
        "@nestjs/serve-static": "2.2.2",
        "@nestjs/swagger": "5.2.1",
        "body-parser": "1.20.0",
        "chance": "^1.1.8",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "dotenv": "16.0.1",
        "express": "4.18.1",
        "express-async-errors": "3.1.1",
        "helmet": "5.1.0",
        "http-status": "1.5.2",
        "morgan": "1.10.0",
        "nest-shared": "^0.3.9",
        "nestjs-undici": "^0.0.41",
        "qs": "6.10.5",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.5.7",
        "swagger-ui-express": "4.4.0",
        "tsyringe": "4.7.0"
    },
    "devDependencies": {
        "@babel/cli": "7.17.10",
        "@babel/core": "7.18.5",
        "@babel/node": "7.18.5",
        "@babel/preset-env": "7.18.2",
        "@compodoc/compodoc": "1.1.19",
        "@nestjs/cli": "8.2.8",
        "@nestjs/schematics": "8.0.10",
        "@nestjs/testing": "8.4.7",
        "@types/body-parser": "1.19.2",
        "@types/cache-manager": "4.0.0",
        "@types/chance": "^1.1.3",
        "@types/compression": "1.7.2",
        "@types/cors": "2.8.12",
        "@types/express": "4.17.13",
        "@types/helmet": "0.0.48",
        "@types/jest": "27.5.2",
        "@types/moment": "2.13.0",
        "@types/mongoose": "5.11.97",
        "@types/morgan": "1.9.3",
        "@types/multer": "1.4.7",
        "@types/node": "16.11.41",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "@typescript-eslint/typescript-estree": "5.29.0",
        "eslint": "8.18.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.0.0",
        "jest": "28.1.1",
        "nodemon": "2.0.18",
        "prettier": "2.7.1",
        "sucrase": "3.21.1",
        "supertest": "6.2.3",
        "ts-jest": "28.0.5",
        "ts-loader": "9.3.1",
        "ts-node": "10.8.1",
        "tsconfig-paths": "4.0.0",
        "typescript": "4.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
